<% if @errors %>
  <div id="error_explanation">
    <ul>
    <% @errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

<%= form_for @doggo do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :breed %>
<%= f.text_field :breed %>

<%= f.label :img_url, "Image URL"%>
<%= f.text_field :img_url %>

<%= f.submit %>
<% end %>